{
  "name": "official-map-data",
  "type": "module",
  "version": "1.0.0",
  "description": "Map Data Json file for official maps",
  "main": "handler.js",
  "scripts": {
    "test": "npm run generate-schema && npm run generate-map-data && NODE_OPTIONS=\"--max-old-space-size=12288 ${NODE_OPTIONS:-}\" jest test/jsonValidation.test.ts",
    "test:quick-play-maps": "jest test/publishQuickPlayMaps.test.ts",
    "generate-jwt": "node --no-warnings --loader ts-node/esm src/scripts/generate-jwt.ts",
    "generate-schema": "node --no-warnings --loader ts-node/esm src/scripts/generate-schema.ts",
    "generate-thumbnails": "node --max-old-space-size=4096 --no-warnings --loader ts-node/esm src/scripts/generate-thumbnails.ts",
    "generate-map-data": "node --no-warnings --loader ts-node/esm src/scripts/generate-map-data.ts",
    "deploy-map-data": "node --no-warnings --loader ts-node/esm src/scripts/deploy-map-data.ts",
    "deploy-map-data-by-sources": "node --no-warnings --loader ts-node/esm src/scripts/deploy-map-data-by-sources.ts",
    "deploy-dev": "serverless deploy --stage local",
    "deploy": "SLS_DEBUG=* serverless deploy --verbose",
    "publish-quick-play-maps": "node --no-warnings --loader ts-node/esm src/scripts/publish-quick-play-maps.ts",
    "terraform": "node dev-prestart.mjs",
    "dev": "npm run terraform && npm run deploy-dev",
    "dev:publish-quick-play-maps": "node --inspect-brk --no-warnings --loader ts-node/esm -r dotenv/config src/scripts/publish-quick-play-maps.ts"
  },
  "author": "Samuel Hilson",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-s3": "^3.540.0",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "datadog-lambda-js": "^8.108.0",
    "dd-trace": "^5.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "exponential-backoff": "^3.1.2",
    "jest": "^29.7.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.2.5",
    "serverless": "^3.38.0",
    "serverless-bundle": "^6.1.0",
    "serverless-localstack": "^1.2.0",
    "serverless-plugin-datadog": "^5.60.0",
    "ts-jest": "^29.1.2",
    "ts-json-schema-generator": "^1.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.540.0",
    "dotenv": "^16.4.5",
    "sharp": "^0.33.3"
  }
}
